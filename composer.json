  "keywords": ["ddd", "shangCube", "sphere"],
  "homepage": "https://www.3vjuyuan.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Team Delta - San Wei Ju Yuan Tech. Ltd.",
